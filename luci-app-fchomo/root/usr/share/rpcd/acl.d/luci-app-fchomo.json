{
	"luci-app-fchomo": {
		"description": "Grant access to fchomo configuration",
		"read": {
			"file": {
				"/etc/fchomo/ruleset/*": [ "read" ],
				"/etc/fchomo/templates/*": [ "read" ],
				"/etc/init.d/fchomo reload *": [ "exec" ],
				"/var/run/fchomo/fchomo.log": [ "read" ],
				"/var/run/fchomo/mihomo-c.log": [ "read" ],
				"/var/run/fchomo/mihomo-s.log": [ "read" ]
			},
			"ubus": {
				"service": [ "list" ],
				"luci.fchomo": [ "*" ]
			},
			"uci": [ "fchomo" ]
		},
		"write": {
			"file": {
				"/etc/fchomo/ruleset/*": [ "write" ],
				"/etc/fchomo/templates/*": [ "write" ]
			},
			"uci": [ "fchomo" ]
		}
	}
}
